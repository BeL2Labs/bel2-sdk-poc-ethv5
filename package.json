{
    "name": "bel2-cli",
    "version": "1.0.0",
    "description": "A CLI tool for interacting with BeL2 SDK",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/index.ts",
        "cli": "node dist/index.js",
        "debug": "node --inspect-brk -r ts-node/register src/index.ts",
        "postinstall": "patch-package"
    },
    "bin": {
        "bel2-cli": "./dist/index.js"
    },
    "author": "Toky Ralala",
    "license": "MIT",
    "dependencies": {
        "@bel2labs/sdk": "^1.0.0",
        "@types/readline-sync": "^1.4.8",
        "commander": "^8.3.0",
        "crypto-browserify": "^3.12.0",
        "dotenv": "^10.0.0",
        "ethers": "5",
        "patch-package": "^6.4.7",
        "readline-sync": "^1.4.10"
    },
    "devDependencies": {
        "@types/commander": "^2.12.2",
        "@types/dotenv": "^8.2.0",
        "@types/node": "^16.11.12",
        "ts-node": "^10.9.2",
        "typescript": "^4.5.4"
    }
}